int maxSubarraySum(int arr[], int n){
    
    // kadane's algoritm
    int curr_sum=0;
    int max_sum=0;
    for (int i=0; i<n; i++){
        curr_sum=curr_sum+arr[i];
        if (curr_sum<0){
            curr_sum=0;
        }
        else if (max_sum < curr_sum){
            max_sum=curr_sum;
        }
    }
    return max_sum;
}
